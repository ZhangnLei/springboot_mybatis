<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.process.dao.TrackerDao">

    <!-- 插入数据库用户表 -->
    <insert id="insertTracker">
        INSERT INTO process_control_core.tracker(is_process,trace_maker,origin_status,after_status,flag)
        VALUES(#{is_process},#{trace_maker},#{origin_status},#{after_status},#{flag})
    </insert>

    <update id="updateTrackerById">
        UPDATE test.t_person SET
        is_process=#{is_process}, trace_maker=#{trace_maker}, origin_status=#{origin_status}, after_status=#{after_status}, flag=#{flag}
        WHERE trace_id=#{trace_id}
    </update>

</mapper>
